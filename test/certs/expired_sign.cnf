HOME			= .
RANDFILE		= ./.rnd

[ req ]
default_bits		= 2048
distinguished_name	= distinguished_name
attributes		= attributes
x509_extensions		= ext 
prompt			= no

[ distinguished_name ]
commonName		= Test Expired Signer

[ attributes ]

[ ext ]
basicConstraints	= CA:false
keyUsage		= digitalSignature
extendedKeyUsage	= critical,codeSigning
